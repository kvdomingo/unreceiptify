{
  "regions": ["sfo1"],
  "env": {
    "PYTHONPATH": "api/"
  },
  "framework": "svelte",
  "outputDirectory": "ui/.vercel",
  "installCommand": "cd ui && bun install",
  "buildCommand": "cd ui && bun run build",
  "functions": {
    "api/api/app.py": {
      "runtime": "@vercel/python@4.5.1",
      "includeFiles": "api/**/*"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/api/app.py"
    }
  ]
}
